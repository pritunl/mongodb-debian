+  int kFlatAsciiStringTag = ASCII_STRING_TYPE;
+  andi(scratch, type, Operand(kFlatAsciiStringMask));
+  cmpi(scratch, Operand(kFlatAsciiStringTag));
+  bne(failure);
+}
