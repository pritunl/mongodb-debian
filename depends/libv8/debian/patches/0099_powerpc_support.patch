+
+void RegExpMacroAssemblerPPC::IfRegisterEqPos(int reg,
+                                              Label* if_eq) {
+  __ LoadP(r3, register_location(reg), r0);
+  __ cmp(r3, current_input_offset());
+  BranchOrBacktrack(eq, if_eq);
+}
