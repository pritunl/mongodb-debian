+void MacroAssembler::InitializeFieldsWithFiller(Register start_offset,
+                                                Register end_offset,
+                                                Register filler) {
+  Label loop, entry;
+  b(&entry);
+  bind(&loop);
+  StoreP(filler, MemOperand(start_offset), r0);
+  addi(start_offset, start_offset, Operand(kPointerSize));
+  bind(&entry);
+  cmp(start_offset, end_offset);
+  blt(&loop);
+}
