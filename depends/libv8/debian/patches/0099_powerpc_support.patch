+void UnaryOpStub::GenerateSmiCodeSub(MacroAssembler* masm,
+                                     Label* non_smi,
+                                     Label* slow) {
+  __ JumpIfNotSmi(r3, non_smi);
+
+  // The result of negating zero or the smallest negative smi is not a smi.
+  __ TestBitRange(r3, kBitsPerPointer - 2, 0, r0);
+  __ beq(slow, cr0);
+
+  // Return '- value'.
+  __ neg(r3, r3);
+  __ Ret();
+}
