+
+bool Assembler::IsNop(Instr instr, int type) {
+  ASSERT((0 == type) || (DEBUG_BREAK_NOP == type));
+  int reg = 0;
+  if (DEBUG_BREAK_NOP == type) {
+    reg = 3;
+  }
+  return instr == (ORI | reg*B21 | reg*B16);
+}
