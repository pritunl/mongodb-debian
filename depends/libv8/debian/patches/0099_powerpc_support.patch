+  __ JumpIfSmi(r4, &miss);
+  CheckPrototypes(receiver, r4, holder, r6, r7, r8, name, &miss);
+
+  GenerateStoreViaSetter(masm(), setter);
+
+  __ bind(&miss);
+  Handle<Code> ic = masm()->isolate()->builtins()->StoreIC_Miss();
